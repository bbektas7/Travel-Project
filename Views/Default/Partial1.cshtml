@using TravelTripProject.Models.Classes
@model List<TravelTripProject.Models.Blog>
<div class="padding">
    <h4>Blogs' Top 10</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {
        <li class="list-bg"><span>@{i++; }@i.</span><a href="/Blog/BlogDetail/@x.Id">@x.Title></li>
        }
    </ul>
    <a href="#" class="link1">See all hotels</a>
</div>