@using TravelTripProject.Models.Classes
@model List<TravelTripProject.Models.Blog>
<div class="padding1">
    <h4>My Best Places</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
        <div class="bot-img">
            <img src="@x.BlogImage" alt="" />
        </div>
        <div class="bot-desc">
            <p>@x.Explanation.Substring(0,75)... </p>
        </div>
        <div class="clear"></div>

        }
    </div>
        @Html.Action("Partial3","Default")
</div>
